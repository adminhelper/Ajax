	var ajaxLoading = function(isLoading){
		if(isLoading == false){
			$("body").waitMe("hide");
		}else{
			$("body").waitMe({
				effect : "ios",
				text : "처리중입니다.",
				bg : 'rgba(255,255,255,0.7)',
				color : '#000',
				source : 'waitme/img.svg'
			});
		}
	}
